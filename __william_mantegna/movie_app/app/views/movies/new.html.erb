<h1>Add a Movie</h1>

<%= form_for @movie do |form| %>
	<%= form.label :title %>
	<%= form.text_field :title %><br />

	<%= form.label :description %>
	<%= form.text_area :description %><br />

	<%= form.label :year_released %>
	<%= form.number_field :year_released %><br />

	<%= form.submit %>

<% end %>