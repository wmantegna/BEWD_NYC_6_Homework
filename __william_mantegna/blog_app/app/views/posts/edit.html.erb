<h1>Edit a post</h1>

<h1>Create a new Post</h1>
<%= form_for @post do |form| %>
	<table>
		<tr>
			<td><%= form.label :title %></td>
			<td><%= form.text_field :title %></td>
		</tr>
		<tr>
			<td><%= form.label :content %></td>
			<td><%= form.text_area :content %></td>
		</tr>
	</table>
	
	<%= form.submit %>
<% end %>